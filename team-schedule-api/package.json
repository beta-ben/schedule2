{
	"name": "team-schedule-api",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"deploy:staging": "wrangler deploy --env staging",
		"kv:create:staging": "wrangler kv namespace create team-schedule-api-staging__SCHEDULE_KV",
		"secrets:staging": "wrangler secret put ADMIN_PASSWORD --env staging && wrangler secret put SITE_PASSWORD --env staging",
		"health:staging": "curl -sS https://team-schedule-api-staging.phorbie.workers.dev/api/_health",
		"get:schedule:staging": "curl -sS https://team-schedule-api-staging.phorbie.workers.dev/api/schedule"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240821.0",
		"wrangler": "^3.90.0"
	}
}
